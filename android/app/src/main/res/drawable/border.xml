<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true">
	    <shape>
			<solid android:color="#6CD1FC"/>
			<stroke android:width="1dp" android:color="#58CBFC"></stroke>
		</shape>
	</item>
	
	<item>
	    <shape>
	        <solid android:color="#ffffff"/>
			<stroke android:width="1dp" android:color="#e1e1e1"></stroke>
		</shape>
	</item>
</selector>